<?xml version="1.0" encoding="UTF-8"?>
<profile>
  <name>Konqueror</name>
  <description>You may select erasing tasks that are related to 'Konqueror'</description>
  <description lang="ru">Вы можете выбрать задания по стиранию, связанные с 'Konqueror'</description>
  <icon>konqueror.png</icon>
  <task>
    <name>Cookies</name>
    <name lang="ru">Куки</name>
    <description>You may erase cookies of 'Konqueror'</description>
    <description lang="ru">Вы можете стереть куки 'Konqueror'</description>
    <icon>cookies.png</icon>
    <action parentDir=".kde/share/apps/kcookiejar" wildCard="cookies"/>
    <action parentDir=".kde4/share/apps/kcookiejar" wildCard="cookies"/>
  </task>
  <task>
    <name>Current Session</name>
    <name lang="ru">Текущая сессия</name>
    <description>You may erase current session of 'Konqueror'</description>
    <description lang="ru">Вы можете стереть текущую сессию 'Konqueror'</description>
    <icon>session.png</icon>
    <action deleteAfter="false" deleteSubDirs="false" parentDir=".kde/share/apps/konqueror" wildCard="autosave"/>
    <action deleteAfter="false" deleteSubDirs="false" parentDir=".kde4/share/apps/konqueror" wildCard="autosave"/>
  </task>
  <task>
    <name>URL History</name>
    <name lang="ru">История URL</name>
    <description>You may erase the history of visited web pages in 'Konqueror'</description>
    <description lang="ru">Вы можете стереть историю посещенных веб-страниц в 'Konqueror'</description>
    <icon>history.png</icon>
    <action parentDir=".kde/share/apps/konqueror" wildCard="closeditems_saved"/>
    <action parentDir=".kde/share/apps/konqueror" wildCard="konq_history"/>
    <action parentDir=".kde/share/config" wildCard="konq_history"/>
    <action parentDir=".kde4/share/apps/konqueror" wildCard="closeditems_saved"/>
    <action parentDir=".kde4/share/apps/konqueror" wildCard="konq_history"/>
    <action parentDir=".kde4/share/config" wildCard="konq_history"/>
  </task>
</profile>
